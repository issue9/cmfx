    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/issue9/cmfx",
        "directory": "admin"
    },
    "keywords": [
        "cmfx",
        "admin",
        "dashboard"
    ],
    "license": "MIT",
    "files": [
        "lib/",
        "README.md",
        "LICENSE"
    ],
    "types": "./lib/index.d.ts",
    "module": "./lib/index.js",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "import": "./lib/index.js"
        },
        "./style.css": {
