            "import": "./lib/style.css"
        },
        "./pages": {
            "types": "./lib/pages.d.ts",
            "import": "./lib/pages.js"
        },
        "./core": {
            "types": "./lib/core.d.ts",
            "import": "./lib/core.js"
        },
        "./messages/": {
            "require": "./lib/messages/",
            "import": "./lib/messages/"
        }
    },
    "scripts": {
        "build": "vite build",
        "test": "vitest --coverage.enabled true",
        "test-nowatch": "vitest run"
    },
    "dependencies": {
        "@formatjs/intl-durationformat": "^0.7.4",
