{
    "name": "@cmfx/admin",
    "version": "0.8.8",
    "type": "module",
    "description": "适用于 cmfx 的后台管理框架",
    "author": {
        "name": "caixw",
        "url": "https://caixw.io"
    },
    "homepage": "https://github.com/issue9/cmfx",
    "bugs": {
        "url": "https://github.com/issue9/cmfx/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/issue9/cmfx",
        "directory": "admin"
    },
    "keywords": [
        "cmfx",
        "admin",
        "dashboard"
    ],
    "license": "MIT",
    "files": [
        "lib/",
        "README.md",
        "LICENSE",
        "tailwind.preset.ts"
    ],
    "types": "./lib/index.d.ts",
    "main": "./lib/index.cjs.js",
    "module": "./lib/index.es.js",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.cjs.js",
            "import": "./lib/index.es.js"
        },
        "./style.css": {
            "require": "./lib/style.css",
            "import": "./lib/style.css"
        },
        "./pages": {
            "types": "./lib/pages.d.ts",
            "require": "./lib/pages.cjs.js",
            "import": "./lib/pages.es.js"
        },
        "./components": {
            "types": "./lib/components.d.ts",
            "require": "./lib/components.cjs.js",
            "import": "./lib/components.es.js"
        },
        "./core": {
            "types": "./lib/core.d.ts",
            "require": "./lib/core.cjs.js",
            "import": "./lib/core.es.js"
        },
        "./messages/": {
            "require": "./lib/messages/",
            "import": "./lib/messages/"
        },
        "./dev": {
            "require": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./dev/demo": {
            "require": "./src/demo.tsx",
            "import": "./src/demo.tsx"
        },
        "./dev/style.css": {
            "require": "./src/style.css",
            "import": "./src/style.css"
        },
        "./dev/pages": {
            "require": "./src/pages/index.ts",
            "import": "./src/pages/index.ts"
        },
        "./dev/components": {
            "require": "./src/components/index.ts",
            "import": "./src/components/index.ts"
        },
        "./dev/core": {
            "require": "./src/core/index.ts",
            "import": "./src/core/index.ts"
        },
        "./dev/messages/": {
            "require": "./src/messages/",
            "import": "./src/messages/"
        }
    },
    "scripts": {
        "build": "vite build",
        "test": "vitest --coverage.enabled true"
    },
    "dependencies": {
        "@formatjs/intl-durationformat": "^0.7.3",
        "@formatjs/intl-localematcher": "^0.6.0",
        "@scure/base": "^1.2.4",
        "@solidjs/router": "^0.15.3",
        "tailwindcss": "^4.0.7",
        "bowser": "^2.11.0",
        "echarts": "^5.6.0",
        "intl-messageformat": "^10.7.15",
        "material-symbols": "^0.28.2",
        "qr-code-styling": "^1.9.1",
        "quill": "^2.0.3",
        "solid-js": "^1.9.4",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
        "yaml": "^2.7.0"
    },
    "devDependencies": {
        "@types/node": "^22.13.4",
        "@vitest/coverage-v8": "^3.0.6",
        "autoprefixer": "^10.4.20",
        "cssnano": "^7.0.6",
        "jsdom": "^26.0.0",
        "postcss": "^8.5.2",
        "@tailwindcss/vite": "^4.0.7",
        "typescript": "^5.7.3",
        "vite": "^6.1.0",
        "vite-plugin-dts": "^4.5.0",
        "vite-plugin-solid": "^2.11.1",
        "vite-plugin-static-copy": "^2.2.0",
        "vitest": "^3.0.6",
        "vitest-fetch-mock": "^0.4.3"
    },
    "overrides": {
        "@asamuzakjp/css-color": "3.0.12"
    }
}